{
  "manifest_version": 2,
  "name": "vuplex-webview-gecko-extension",
  "version": "1.0",
  "description": "Adds support for the Vuplex 3D WebView window.vuplex API.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "index.jsm"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "nativeMessaging",
    "geckoViewAddons"
  ]
}
